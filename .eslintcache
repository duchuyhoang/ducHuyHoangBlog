    {tags?.map((tag, index) => (\n                <>\n                  <Link href={`/tag?tag=${tag}`} passHref key={`tag_${index}`}>\n                    <a>{tag}</a>\n                  </Link>\n                  {index !== tags.length - 1 && <span>,</span>}\n                </>\n              ))}\n            </div>\n            <div className=\"col-md-3 col-12 d-flex align-center justify-content-md-end\">\n              <span className=\"mr-1 mb-1\">\n                <BiCalendar />\n              </span>\n              <p className=\"date mb-0\">{moment(date).format('YYYY-MM-DD')}</p>\n            </div>\n          </div>\n          <div className=\"description\">\n            {description}\n            {/* <ShowMoreText lines={4}>{description}</ShowMoreText> */}\n          </div>\n        </div>\n      </div>\n    </div>\n  )\n}\n\nexport default HorizontalCardPost\n","/home/huyhoang/Desktop/code/ducHuyHoangBlog/components/layout/LeftSideBar.tsx",[],[],[],"/home/huyhoang/Desktop/code/ducHuyHoangBlog/redux/reducer/index.ts",[],[],"/home/huyhoang/Desktop/code/ducHuyHoangBlog/hooks/useMediaQuery.tsx",[],["353"],"/home/huyhoang/Desktop/code/ducHuyHoangBlog/redux/reducer/user.ts",[],[],"/home/huyhoang/Desktop/code/ducHuyHoangBlog/pages/_document.tsx",[],[],"/home/huyhoang/Desktop/code/ducHuyHoangBlog/pages/404.tsx",["354"],[],"/home/huyhoang/Desktop/code/ducHuyHoangBlog/pages/about.tsx",["355"],[],"import Link from 'next/link'\nimport React, { useCallback, useEffect, useState } from 'react'\nimport { AiOutlineMail } from 'react-icons/ai'\nimport { FaFacebookSquare, FaGithub, FaLinkedin } from 'react-icons/fa'\nimport HoverEffect from 'hover-effect'\nimport useDocumentTitle from '../common/hooks/useDocumentTitle'\n\nenum Person {\n  ME = 'ME',\n  LOVE = 'LOVE'\n}\n\nconst About = () => {\n  // eslint-disable-next-line @typescript-eslint/no-explicit-any\n  const [hoverEffect, setHoverEffect] = useState<any>(null)\n  const [currentPerson, setCurrentPerson] = useState<Person>(Person.ME)\n\n  useDocumentTitle({\n    title: 'About me'\n  })\n\n  const handleClick = useCallback(() => {\n    if (currentPerson === Person.ME) {\n      hoverEffect.next()\n      setCurrentPerson(Person.LOVE)\n    } else {\n      hoverEffect.previous()\n      setCurrentPerson(Person.ME)\n    }\n  }, [hoverEffect, currentPerson])\n\n  useEffect(() => {\n    const hover = new HoverEffect({\n      parent: document.querySelector('#userAvatar'),\n      intensity: 0.5,\n      image1: './self.jpg',\n      image2: './logo.jpg',\n      displacementImage: './distortion.png'\n    })\n    setHoverEffect(hover)\n  }, [])\n\n  return (\n    <section className=\"about w-100 mt-4\">\n      <div className=\"about-tab-container w-100 d-flex\">\n        <div className=\"about-content\">\n          <div\n            id=\"userAvatar\"\n            className=\"userAvatar\"\n            // style={{\n            //   backgroundImage: \"url('./self.jpg')\",\n            // }}\n          ></div>\n          {currentPerson === Person.ME && (\n            <div className=\"col-12 col-md-6 col-lg-6 col-xl-6 p-2 h-100 description\">\n              <div className=\"name-wrapper\">\n                <h1 className=\"description-name\">Hoàng Đức Huy</h1>\n                <sub>I do backend, frontend React, Vue.</sub>\n              </div>\n              <div className=\"description-wrapper\">\n                <p className=\"mb-3\">\n                  Các bạn có thể gọi mình là Huy ^^.Mình từng là sinh viên của\n                  PTIT. Mình viết blog này với mục đích lưu trữ lại kiến thức\n                  với những công nghệ mình đã tìm hiểu. Vậy nên nếu bạn muốn\n                  đóng góp gì hãy liên lạc với mình qua thông tin bên dưới\n                </p>\n                <q>\n                  <i style={{ paddingRight: '2px' }}>\n                    To infinity and beyond 🚀\n                  </i>\n                </q>\n                <div className=\"col-12 row align-center social-wrapp